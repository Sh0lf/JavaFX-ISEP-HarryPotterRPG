<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.hpah.views.GUI.controller.DungeonPresentationController">
   <children>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0">
         <children>
            <TextFlow fx:id="dungeonName" prefHeight="59.0" prefWidth="600.0">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </TextFlow>
            <TextFlow fx:id="dungeonDesc" prefHeight="250.0" prefWidth="600.0">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </TextFlow>
            <Button fx:id="confirmButton" mnemonicParsing="false" text="Confirm">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
